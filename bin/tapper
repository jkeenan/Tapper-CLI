#! perl

use App::Rad;
use Tapper::CLI::Notification;
use Tapper::CLI::Testplan;
use Tapper::CLI::Schema;
use Tapper::CLI::User;


use strict;
use warnings;
use 5.010;


sub setup
{
        my ($c) = @_;
        Tapper::CLI::Notification::setup($c);
        Tapper::CLI::Schema::setup($c);
        Tapper::CLI::Testplan::setup($c);
        Tapper::CLI::User::setup($c);
}


App::Rad->run();

